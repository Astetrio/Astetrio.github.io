  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "pm2 start quasar --name dev -- dev",
    "debug": "quasar dev",
    "build": "quasar build -P",
    "deploy": "git subtree split --prefix dist/spa -b gh-pages && git push -f origin gh-pages:main && git branch -D gh-pages"
  },
